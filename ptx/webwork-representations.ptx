a.  [`\forall x \exists y P(x,y)\text{.}`] [_]{$false}{5}

a.  [`\forall y \exists x P(x,y)\text{.}`] [_]{$true}{5}

a.  [`\exists x \forall y P(x,y)\text{.}`] [_]{$true}{5}

a.  [`\exists y \forall x P(x,y)\text{.}`] [_]{$false}{5}

END_PGML

